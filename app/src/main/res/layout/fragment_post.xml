<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MapaH"
    android:background="@drawable/fondoniveles"
    android:orientation="vertical">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
    <Button
        android:textColor="@color/black"
        android:id="@+id/boton1"
        android:layout_marginTop="15dp"
        android:gravity="left"
        android:paddingStart="10dp"
        android:paddingBottom="5dp"
        android:layout_marginStart="15dp"
        android:text="\u003C        "
        android:textSize="40sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/rellenables"
        tools:ignore="RtlHardcoded" />
    <View
        android:id="@+id/container1"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@color/black"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/boton1"
        />
    <ImageView
        android:id="@+id/perfil1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/perfil"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/container1"
        app:layout_constraintStart_toStartOf="@id/container1" />
    <TextView
        android:id="@+id/nombre1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dokutah 0 sanity"
        android:textColor="@color/white"
        android:layout_marginTop="20dp"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/crimson"
        app:layout_constraintTop_toTopOf="@id/container1"
        app:layout_constraintStart_toEndOf="@id/perfil1" />
    <TextView
        android:id="@+id/context1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meme"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/crimson"
        app:layout_constraintTop_toBottomOf="@id/nombre1"
        app:layout_constraintStart_toEndOf="@id/perfil1" />
    <TextView
        android:id="@+id/seguir1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/seguir"
        android:text="Seguir"
        android:textColor="@color/white"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@id/container1"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <ImageView
        android:id="@+id/meme1"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:src="@drawable/everyone"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/context1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <ImageView
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/view"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toBottomOf="@id/meme1"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/tview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100 mil"
        android:textColor="@color/white"
        android:fontFamily="@font/crimson"
        android:textSize="20sp"
        android:layout_marginTop="17dp"
        android:layout_marginStart="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme1"
        app:layout_constraintStart_toEndOf="@id/view1"
        />
    <TextView
        android:id="@+id/tlike1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="40 mil"
        android:textColor="@color/white"
        android:fontFamily="@font/crimson"
        android:textSize="20sp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme1"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <ImageView
        android:id="@+id/like1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/like"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme1"
        app:layout_constraintEnd_toStartOf="@id/tlike1" />
    <TextView
        android:id="@+id/tcoment1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10 mil"
        android:textColor="@color/white"
        android:fontFamily="@font/crimson"
        android:textSize="20sp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme1"
        app:layout_constraintEnd_toStartOf="@id/like1" />
    <ImageView
        android:id="@+id/coment1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/coment"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme1"
        app:layout_constraintEnd_toStartOf="@id/tcoment1" />
    <View
        android:id="@+id/container2"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@color/black"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/container1"
        />
    <ImageView
        android:id="@+id/perfil2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/perfil"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/container2"
        app:layout_constraintStart_toStartOf="@id/container2" />
    <TextView
        android:id="@+id/nombre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dokutah 0 sanity"
        android:textColor="@color/white"
        android:layout_marginTop="20dp"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/crimson"
        app:layout_constraintTop_toTopOf="@id/container2"
        app:layout_constraintStart_toEndOf="@id/perfil2" />
    <TextView
        android:id="@+id/context2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meme"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/crimson"
        app:layout_constraintTop_toBottomOf="@id/nombre2"
        app:layout_constraintStart_toEndOf="@id/perfil2" />
    <TextView
        android:id="@+id/seguir2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/seguir"
        android:text="Seguir"
        android:textColor="@color/white"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@id/container2"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <ImageView
        android:id="@+id/meme2"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:src="@drawable/everyone"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/context2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <ImageView
        android:id="@+id/view2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/view"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toBottomOf="@id/meme2"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/tview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100 mil"
        android:textColor="@color/white"
        android:fontFamily="@font/crimson"
        android:textSize="20sp"
        android:layout_marginTop="17dp"
        android:layout_marginStart="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme2"
        app:layout_constraintStart_toEndOf="@id/view2"
        />
    <TextView
        android:id="@+id/tlike2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="40 mil"
        android:textColor="@color/white"
        android:fontFamily="@font/crimson"
        android:textSize="20sp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme2"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <ImageView
        android:id="@+id/like2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/like"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme2"
        app:layout_constraintEnd_toStartOf="@id/tlike2" />
    <TextView
        android:id="@+id/tcoment2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10 mil"
        android:textColor="@color/white"
        android:fontFamily="@font/crimson"
        android:textSize="20sp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme2"
        app:layout_constraintEnd_toStartOf="@id/like2" />
    <ImageView
        android:id="@+id/coment2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/coment"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintTop_toBottomOf="@id/meme2"
        app:layout_constraintEnd_toStartOf="@id/tcoment2" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>